! def2-SVP TightSCF

%output
  print[p_mos] true    # print MO coefficients
  print[p_basis] 5       # write <jobname>.molden.input
end

%moinp "d3h.gbw"

%casscf
  nel    2          # CAS(2,2): 2 active electrons
  norb   2          #          in 2 active orbitals

  mult   1,3        # first block: singlet (1), second block: triplet (3)
  nroots 1,1        # 1 singlet root + 1 triplet root

  PTMethod FIC_CASPT2

  PTSettings
    CASPT2_ishift    0.0
    CASPT2_rshift    0.0
    CASPT2_IPEAshift 0.0
  end
end

* xyz 0 3
  C    0.00000    0.00000   -2.25000
  H    1.08250    0.00000   -2.25000
  H   -0.54125    0.93740   -2.25000
  H   -0.54125   -0.93740   -2.25000
  C    0.00000    0.00000    2.25000
  H    1.08250    0.00000    2.25000
  H   -0.54125    0.93740    2.25000
  H   -0.54125   -0.93740    2.25000
*
